---
import { getEntries } from "src/lib/garden/entries";
import { entryTypeIds } from "garden.config";
import GardenEntryCard from "@components/garden/GardenEntryCard.astro";

export async function getStaticPaths() {
  const entries = await getEntries([...entryTypeIds]);

  return entries.map((entry) => ({
    params: { id: entry.id },
    props: { entry },
  }));
}

const { entry } = Astro.props;
---

<GardenEntryCard content={entry} />
